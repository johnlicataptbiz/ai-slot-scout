{
  "manifest_version": 3,
  "name": "AI Slot Scout",
  "description": "Find available scheduling slots from a public Calendly/HubSpot page using an AI assistant.",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "AI Slot Scout"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
